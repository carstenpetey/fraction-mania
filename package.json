{
  "name": "fraction-mania",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --cache --report-unused-disable-directives . && prettier --check .",
