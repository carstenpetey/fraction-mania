    "konva": "^10.0.8",
    "tailwindcss": "^4.1.15"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.10.1",
    "eslint": "^9.38.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vitest": "^4.0.8"
  }
}
